<ContentControl
    x:Class="NoCap.Library.Controls.CommandFactorySelector"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Name="selector"
    Focusable="False">
    <ComboBox DisplayMemberPath="Name" Name="commandFactoryList"
        SelectedItem="{Binding ElementName=selector, Path=CommandFactory}"
        VerticalAlignment="Stretch"
        HorizontalAlignment="Stretch" />
</ContentControl>
