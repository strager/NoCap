<ContentControl
    x:Class="NoCap.Library.Controls.CommandSelector"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Name="selector"
    Focusable="False">
    <ComboBox DisplayMemberPath="Name" Name="commandList" SelectedItem="{Binding ElementName=selector, Path=Command}" />
</ContentControl>
