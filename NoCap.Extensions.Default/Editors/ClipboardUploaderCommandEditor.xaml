<ContentControl x:Class="NoCap.Extensions.Default.Editors.ClipboardUploaderCommandEditor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:NoCap.Library.Controls;assembly=NoCap.Library"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel Orientation="Vertical">
            <local:FramedCommandEditor Header="Image uploader"
                Command="{Binding ImageUploader}"
                Filter="ImageUploader" />

            <local:FramedCommandEditor Header="Text uploader"
                Command="{Binding TextUploader}"
                Filter="TextUploader" />

            <local:FramedCommandEditor Header="URL shortener"
                Command="{Binding UrlShortener}"
                Filter="UrlShortener" />

            <local:FramedCommandEditor Header="File uploader"
                Command="{Binding FileUploader}"
                Filter="FileUploader" />
        </StackPanel>
    </Grid>
</ContentControl>
